{
  "extends": [
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "prettier"
  ],
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "arrow-function",
        "unnamedComponents": "arrow-function"
      }
    ],
    "jsx-a11y/label-has-associated-control": [
      0
    ],
    "default-param-last": [
      0
    ]
  },
  "plugins": [
    "prettier"
  ]
}